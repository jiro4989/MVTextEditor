<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<?import app.manager.TextManager?>
<?import app.menubar.MyMenuBar?>
<?import app.table.TextTable?>
<?import app.viewer.TextViewer?>

<BorderPane fx:controller="app.MainController" prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">

  <top>
    <MyMenuBar fx:id="myMenuBar" BorderPane.alignment="CENTER" />
  </top>

  <center>
    <SplitPane dividerPositions="0.6868686868686869" BorderPane.alignment="CENTER">
      <items>

        <AnchorPane minHeight="0.0" minWidth="0.0">
          <children>
            <BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
              <center>
                <TextTable fx:id="textTable" BorderPane.alignment="CENTER" />
              </center>
              <bottom>
                <AnchorPane BorderPane.alignment="CENTER" >
                  <children>
                    <TextViewer fx:id="textViewer" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="10.0" />
                  </children>
                </AnchorPane>
              </bottom>
            </BorderPane>
          </children>
        </AnchorPane>

        <AnchorPane minHeight="0.0" minWidth="0.0">
          <children>
            <TextManager fx:id="textManager" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
          </children>
        </AnchorPane>

      </items>
    </SplitPane>
  </center>
</BorderPane>
